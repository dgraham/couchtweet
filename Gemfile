source :rubygems

gem 'bcrypt-ruby', :require => 'bcrypt'
gem 'couchrest_model'
gem 'jquery-rails'
gem 'mustache-rails', :require => 'mustache/railtie'
gem 'rails', '3.2.8'
gem 'resque'
gem 'thin'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
end

group :development do
  gem 'foreman'
  gem 'rake'
end

group :test do
  gem 'minitest-rails'
end

